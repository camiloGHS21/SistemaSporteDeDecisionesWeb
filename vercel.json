{
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@vercel/docusaurus",
      "config": { "project": "docs", "root": "docs" }
    },
    {
      "src": "package.json",
      "use": "@vercel/vite",
      "config": { "project": "SistemaSporteDeDecisionesWeb", "root": "." }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/docs",
      "dest": "/docs/"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
